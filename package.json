{
  "name": "aws-testing-library",
  "version": "4.0.6",
  "description": "Chai and Jest matchers for aws services",
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "format": "prettier --write src/**/*.ts",
    "format:ci": "prettier --list-different src/**/*.ts",
    "prepare": "npm run build"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "serverless",
    "testing",
    "aws",
    "jest",
    "chai",
    "mocha",
    "s3",
    "dynamoDb",
    "api",
    "cloudwatch",
    "kinesis",
    "lambda",
    "sqs",
    "sns",
    "step-function"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erezrokah/aws-testing-library.git"
  },
  "homepage": "https://github.com/erezrokah/aws-testing-library/#readme",
  "author": "Erez Rokah",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/chai": "^4.2.12",
    "@types/jest": "^29.5.11",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "chai": "^4.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "mockdate": "^3.0.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.478.0",
    "@aws-sdk/client-dynamodb": "^3.478.0",
    "@aws-sdk/client-kinesis": "^3.478.0",
    "@aws-sdk/client-lambda": "^3.478.0",
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/client-sfn": "^3.478.0",
    "@aws-sdk/client-sns": "^3.478.0",
    "@aws-sdk/client-sqs": "^3.478.0",
    "@aws-sdk/lib-dynamodb": "^3.478.0",
    "axios": "^1.6.2",
    "filter-obj": "^3.0.0",
    "jest-diff": "^29.7.0",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=16.10.0"
  }
}
